"resource/layout/twofactorcodechallenge.layout"
{
	controls
	{
		TwoFactorCodeChallenge { controlname="Frame" title="#Steam_GetTwoFactorCode_Title" style="TwoFactorCodeChallenge" }

		Label1 { controlname="Label" wrap=1 style="Instructions" labeltext="#Steam_GetTwoFactorCode_EnterCode" }
		Label2 { controlname="Label" wrap=1 style="Instructions" labeltext="#Steam_GetTwoFactorCode_EnterCodeDetails" }

		Key { controlname="Label" style="Key" }
		AuthCode { controlname="TextEntry" style="AuthCode" command="AuthCodeEntered" tabposition=1 }
		Background { controlname="ImagePanel" style="Background" image="graphics/bg_security_wizard" zpos=-4 }

		OK { controlname="Button" tabposition=2 command="AuthCodeEntered" labeltext="#PropertyDialog_OK" default=1 enabled=false }
		Cancel { controlname="Button" tabposition=3 command="Close" labeltext="#PropertyDialog_Cancel" }
	}

	styles
	{

		Button {

			image="logindialog/twofactorchallenge/checkbutton"
			textcolor=none
			padding-bottom=11
			padding-top=11
			padding-left=111
			padding-right=111
			render_bg
			{
		
				0="fill(x0+2,y0+2,x1-2,y1-2,buttona)"

				///////////////////esquina izquierda inferior/////////////////////////////
				1="image(x0-1,y1-4,x0+2,y1+4, bordescurvos/b/ai)"

				////////////////////parte inferior/////////////////////////////////////////
				2="image_tiled(x0+4,y1-2,x1-4,y1+4, bordescurvos/b/am)"
				
                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, bordescurvos/b/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0-1,y0-0,x0+2,y0+3, bordescurvos/b/ti)"

                ////////////////////////Parte del medio superior//////////////////////////////
				6="image_tiled(x0+4,y0-2,x1-4,y0+2, bordescurvos/b/tm)"

                //////////////medio derecha/////////////////////////////////////////////////////////////////////
				4="image_tiled(x1-2,y0+4,x1+3,y1-4, bordescurvos/b/md)"

				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-4,y0-0,x1+3,y0+2, bordescurvos/b/td)"

				/////////////////////esquina inferior derecha//////////////////////////////////////////////////////
				8="image(x1-4,y1-4,x1+3,y1+2,bordescurvos/b/ad)"
			
			
			}
			

		}
		Button:hover {

			textcolor=none
			render_bg
			{
		
				0="fill(x0+2,y0+2,x1-2,y1-2,checkbutton_hover)"

				///////////////////esquina izquierda inferior/////////////////////////////
				1="image(x0-1,y1-4,x0+2,y1+4, bordescurvos/twofactorchallenge/hover/ai)"

				////////////////////parte inferior/////////////////////////////////////////
				2="image_tiled(x0+4,y1-2,x1-4,y1+4, bordescurvos/twofactorchallenge/hover/am)"
				
                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, bordescurvos/twofactorchallenge/hover/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0-1,y0-0,x0+2,y0+3, bordescurvos/twofactorchallenge/hover/ti)"

                ////////////////////////Parte del medio superior//////////////////////////////
				6="image_tiled(x0+4,y0-2,x1-4,y0+2, bordescurvos/twofactorchallenge/hover/tm)"

                //////////////medio derecha/////////////////////////////////////////////////////////////////////
				4="image_tiled(x1-2,y0+4,x1+3,y1-4, bordescurvos/twofactorchallenge/hover/md)"

				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-4,y0-0,x1+3,y0+2, bordescurvos/twofactorchallenge/hover/td)"

				/////////////////////esquina inferior derecha//////////////////////////////////////////////////////
				8="image(x1-4,y1-4,x1+3,y1+2,bordescurvos/twofactorchallenge/hover/ad)"
			
			
			}
			

		}

		Button:active {

			textcolor=none
			render_bg
			{
		
				0="fill(x0+2,y0+2,x1-2,y1-2,checkbutton_active)"

				///////////////////esquina izquierda inferior/////////////////////////////
				1="image(x0-1,y1-4,x0+2,y1+4, bordescurvos/twofactorchallenge/selected/ai)"

				////////////////////parte inferior/////////////////////////////////////////
				2="image_tiled(x0+4,y1-2,x1-4,y1+4, bordescurvos/twofactorchallenge/selected/am)"
				
                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, bordescurvos/twofactorchallenge/selected/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0-1,y0-0,x0+2,y0+3, bordescurvos/twofactorchallenge/selected/ti)"

                ////////////////////////Parte del medio superior//////////////////////////////
				6="image_tiled(x0+4,y0-2,x1-4,y0+2, bordescurvos/twofactorchallenge/selected/tm)"

                //////////////medio derecha/////////////////////////////////////////////////////////////////////
				4="image_tiled(x1-2,y0+4,x1+3,y1-4, bordescurvos/twofactorchallenge/selected/md)"

				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-4,y0-0,x1+3,y0+2, bordescurvos/twofactorchallenge/selected/td)"

				/////////////////////esquina inferior derecha//////////////////////////////////////////////////////
				8="image(x1-4,y1-4,x1+3,y1+2,bordescurvos/twofactorchallenge/selected/ad)"
			
			
			}

		}	
		TwoFactorCodeChallenge {

			minimum-height=540
			minimum-width=380

				render_bg {

				1="image(x0,y0,x1,y1, logindialog/twofactorchallenge/steamguard)"
				

			}
			
		}

				textcolor=none
			render_bg {

				0="image(x0,y0,x1,y1, logindialog/twofactorchallenge/shieldkeyhole)"
			}

		AuthCode
		{
			
			font-size=58
			inset="60 -4 0 0 "
			font-family=button
			bgcolor="none"
			render_bg
			{
				0="fill(x0+2,y0+2,x1-2,y1-2,textentrada)"

				///////////////////esquina izquierda inferior/////////////////////////////
				1="image(x0-1,y1-4,x0+2,y1+4, bordescurvos/textentry/ai)"

				////////////////////parte inferior/////////////////////////////////////////
				2="image_tiled(x0+4,y1-2,x1-4,y1+4, bordescurvos/textentry/am)"
				
                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, bordescurvos/textentry/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0-1,y0-0,x0+2,y0+3, bordescurvos/textentry/ti)"

                ////////////////////////Parte del medio superior//////////////////////////////
				6="image_tiled(x0+4,y0-2,x1-4,y0+2, bordescurvos/textentry/tm)"

                //////////////medio derecha/////////////////////////////////////////////////////////////////////
				4="image_tiled(x1-2,y0+4,x1+3,y1-4, bordescurvos/textentry/md)"

				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-4,y0-0,x1+3,y0+2, bordescurvos/textentry/td)"

				/////////////////////esquina inferior derecha//////////////////////////////////////////////////////
				8="image(x1-4,y1-4,x1+3,y1+2,bordescurvos/textentry/ad)"
			}
		}
		
			Key
		{
		}
		
		Instructions
		{
			font-size=18
			font-family=button
			inset="12 10 0 0"
			render_bg
			{
		
				0="fill(x0+2,y0+2,x1-2,y1-2,buttona)"

				///////////////////esquina izquierda inferior/////////////////////////////
				1="image(x0-1,y1-4,x0+2,y1+4, bordescurvos/b/ai)"

				////////////////////parte inferior/////////////////////////////////////////
				2="image_tiled(x0+4,y1-2,x1-4,y1+4, bordescurvos/b/am)"
				
                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, bordescurvos/b/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0-1,y0-0,x0+2,y0+3, bordescurvos/b/ti)"

                ////////////////////////Parte del medio superior//////////////////////////////
				6="image_tiled(x0+4,y0-2,x1-4,y0+2, bordescurvos/b/tm)"

                //////////////medio derecha/////////////////////////////////////////////////////////////////////
				4="image_tiled(x1-2,y0+4,x1+3,y1-4, bordescurvos/b/md)"

				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-4,y0-0,x1+3,y0+2, bordescurvos/b/td)"

				/////////////////////esquina inferior derecha//////////////////////////////////////////////////////
				8="image(x1-4,y1-4,x1+3,y1+2,bordescurvos/b/ad)"
			
			
			}
		}
		
		Background
		{
			render_bg{}
		}
	}

	layout
	{
		

		
		place {

			control="Label2"
			margin-top=90
			width=352
			height=72
			margin-left=14

		}

		place { 
			
			control="Key" 
			margin-top=186 
			margin-left=14
			
		}

		place { 
			
			control="AuthCode" 
			align=bottom 
			margin-left=112 
			margin-bottom=176 
			width=152 height=58 
			
		}

		// confirm button
		place {
			
			control="Ok" 
			margin-left=57 
			margin-bottom=40 
			align=bottom 
			
		}

		// hidden
		place { control="Cancel,Label1" align=bottom margin-right=9999 }
	}
}