"resource/layout/htmlpopup.layout"
{
	styles
	{
		BackButtonSm
		{
			bgcolor="none"
			render_bg{}
			image="graphics/fluent/icons/navbar/back"
			
		}
		BackButtonSm:hover
		{
			bgcolor=none
			image="graphics/fluent/icons/navbar/back_h"
		}
		BackButtonSm:active
		{
			bgcolor=none
			image="graphics/fluent/icons/navbar/back_p"
		}
		BackButtonSm:disabled
		{
			image="graphics/fluent/icons/navbar/back_d"
		}
		
		ForwardButtonSm
		{
			bgcolor="none"
			render_bg{}
			image="graphics/fluent/icons/navbar/forward"
			
		}
		ForwardButtonSm:hover
		{
			bgcolor=none
			image="graphics/fluent/icons/navbar/forward_h"
		}
		ForwardButtonSm:active
		{
			bgcolor=none
			image="graphics/fluent/icons/navbar/forward_p"
		}
		ForwardButtonSm:disabled
		{
			image="graphics/fluent/icons/navbar/forward_d"
		}
		URLBarSecure
		{
		font-family=cachetazo
		textcolor="white"
		font-size=17
		}
		LabelEVCert
		{
		font-family=cachetazo
		font-size=17
		textcolor="SecureURL"
		inset="6 0 0 2"
		render_bg
			{
				0="fill(x0+2,y0+2,x1-2,y1-2,textentrada)"

				///////////////////esquina izquierda inferior/////////////////////////////
				1="image(x0-1,y1-4,x0+2,y1+4, bordescurvos/textentry/ai)"

				////////////////////parte inferior/////////////////////////////////////////
				2="image_tiled(x0+4,y1-2,x1-4,y1+4, bordescurvos/textentry/am)"
				
                ////////////medio de parte izquierda///////////////////////////////////////////////////
				3="image_tiled(x0-3,y0+4,x0+2,y1-4, bordescurvos/textentry/mi)"
				
				//////////////////////////esquina izquierda superior///////////////////////////////////////////
				5="image(x0-1,y0-0,x0+2,y0+3, bordescurvos/textentry/ti)"

                ////////////////////////Parte del medio superior//////////////////////////////
				6="image_tiled(x0+4,y0-2,x1-4,y0+2, bordescurvos/textentry/tm)"

                //////////////medio derecha/////////////////////////////////////////////////////////////////////
				4="image_tiled(x1-2,y0+4,x1+3,y1-4, bordescurvos/textentry/md)"

				/////////////////////////esquina derecha superior///////////////////////////////////////////////
				7="image(x1-4,y0-0,x1+3,y0+2, bordescurvos/textentry/td)"

				/////////////////////esquina inferior derecha//////////////////////////////////////////////////////
				8="image(x1-4,y1-4,x1+3,y1+2,bordescurvos/textentry/ad)"
			}
		
			
		}
			
	}
	layout
	{
		//Controls
		
		place { 

			control="BackButton,ForwardButton" 
			y=40 
			x=1 
			align=left 
			width=40 
			height=38
			
		}
		
		place { 

			
			 start="ForwardButton"
			 control="EVCert" 
			 height=25	
			 margin-top=7 
			 width=105
			 margin-left=3
			 
			 
		}
			 
		
		place {
			
			 start=EVCert
			 control="URLBar" 
			 align=left 
			 width=max 
			 height=25
			 margin-left=6
			 margin-right=77 
			
			
		}
		
		place { 
			control="PageLoadThrobber,ReloadButton,StopButton" 
			start="URLBar" 
			align=right 
			height=16
			width=16 
			
		}

		//Page
		place { 

			control="HTMLPopupChild" 
			width=max 
			height=max
			y=33
				
		}

		//Hidden

		place { 
			
			control="StatusText,URLStatusImage" 
			margin-left=9787 
			
		}
	}
}